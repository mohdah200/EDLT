   "source": [
    "def calculate_threshold(differential_entropies):\n",
    "    # Calculate the 95th percentile of the differential entropies\n",
    "    threshold = torch.quantile(differential_entropies, 0.95)\n",
    "    return threshold\n",
    "\n",
    "\n"
   ]

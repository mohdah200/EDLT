   "source": [
    "def calculate_threshold(differential_entropies):\n",
    "    mean_entropy = differential_entropies.mean()\n",
    "    std_entropy = differential_entropies.std()\n",
    "    threshold = mean_entropy + 2 * std_entropy\n",
    "    return threshold\n",
    "\n",
    "\n"
   ]
  },
